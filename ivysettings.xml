<?xml version="1.0" encoding="UTF-8"?>
<ivy-settings>
  <settings defaultResolver="artifactory" />
  <!--Authentication required for publishing (deployment). 'Artifactory Realm' is the realm used by Artifactory so don't change it.-->
  <credentials host="artifactory.bpm-id.com" realm="Artifactory Realm" username="anonymous" />
  <resolvers>
    <url name="artifactory-public-release">
        <artifact pattern=
          "http://artifactory.bpm-id.com/artifactory/oe-public-release/[organization]/[module]/[revision]/[artifact]-[revision].[ext]"/>
        <ivy pattern="http://artifactory.bpm-id.com/artifactory/oe-public-release/[organization]/[module]/[revision]/ivy-[revision].xml" />
    </url>
<!--     <url name="artifactory-public-snapshots">
        <artifact pattern=
          "http://artifactory.bpm-id.com/artifactory/oe-public-snapshots/[organization]/[module]/[revision]/[artifact]-[revision].[ext]"/>
        <ivy pattern="http://artifactory.bpm-id.com/artifactory/oe-public-snapshots/[organization]/[module]/[revision]/ivy-[revision].xml" />
    </url> -->
    <chain name="artifactory">
        <url ref="artifactory-public-release" />
<!--        <url ref="artifactory-snapshots" /> -->
    </chain>
  </resolvers>
</ivy-settings>
